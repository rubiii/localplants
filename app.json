{
  "expo": {
    "name": "Local Plants",
    "slug": "localplants",
    "owner": "rubiii",
    "orientation": "portrait",
    "version": "0.0.1",
    "icon": "./assets/icon.png",
    "scheme": "localplants",
    "userInterfaceStyle": "automatic",
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "app.localplants",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSPhotoLibraryUsageDescription": "Need library access please"
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.VIBRATE"
      ],
      "package": "app.localplants"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Camera",
            "PhotoLibrary"
          ]
        }
      ],
      "expo-sqlite",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f4589529-4aa8-4231-8aff-9281ac83de0c"
      }
    }
  }
}
