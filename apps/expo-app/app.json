{
  "expo": {
    "name": "Local Plants",
    "slug": "localplants",
    "owner": "rubiii",
    "orientation": "default",
    "version": "0.0.1",
    "icon": "./assets/icon.png",
    "scheme": "localplants",
    "userInterfaceStyle": "automatic",
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "app.localplants",
      "icon": "./assets/ios.icon",
      "infoPlist": {
        "UIDesignRequiresCompatibility": true,
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSPhotoLibraryUsageDescription": "Need library access please"
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.VIBRATE"
      ],
      "package": "app.localplants"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "react-native-permissions",
        {
          "iosPermissions": ["Camera", "PhotoLibrary"]
        }
      ],
      "expo-sqlite",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fafaf9",
          "image": "./assets/icon.png",
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#292524"
          },
          "imageWidth": 300
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f4589529-4aa8-4231-8aff-9281ac83de0c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f4589529-4aa8-4231-8aff-9281ac83de0c"
    }
  }
}
