{
  "name": "@localplants/expo-app",
  "description": "Pl@ntNet id worker for Local Plants",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "prebuild": "expo prebuild",
    "build:ios:prod": "eas build --clear-cache --platform ios --profile production --local --non-interactive && mkdir -p builds/prod && mv build-* builds/prod/.",
    "build:ios:preview": "eas build --clear-cache --platform ios --profile preview --local --non-interactive && mkdir -p builds/preview && mv build-* builds/preview/.",
    "build:ios:dev": "eas build --clear-cache --platform ios --profile development --local --non-interactive && mkdir -p builds/dev && mv build-* builds/dev/.",
    "build:android:preview": "eas build --clear-cache --platform android --profile preview --local && mkdir -p builds/preview && mv build-* builds/preview/.",
    "build:android:dev": "eas build --clear-cache --platform android --profile development --local && mkdir -p builds/dev && mv build-* builds/dev/.",
    "build:web:dev": "expo export --clear --dev --no-minify --source-maps -p web --max-workers 8 --output-dir builds/web/public && cp spa-server.js builds/web",
    "build:web:prod": "expo export --clear -p web --max-workers 8 --output-dir builds/web/public && cp spa-server.js builds/web",
    "start": "expo start --reset-cache",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "type-check": "tsc"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@bacons/text-decoder": "^0.0.0",
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@expo-google-fonts/anton": "^0.4.2",
    "@expo-google-fonts/calistoga": "^0.4.1",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@likashefqet/react-native-image-zoom": "^4.3.0",
    "@localplants/jazz": "*",
    "@localplants/utils": "*",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@shopify/flash-list": "2.0.2",
    "clsx": "^2.1.1",
    "expo": "catalog:",
    "expo-blur": "~15.0.7",
