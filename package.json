{
  "name": "localplants",
  "version": "0.0.1",
  "main": "index.ts",
  "scripts": {
    "prebuild": "expo prebuild",
    "build:ios:prod": "npx eas build --platform ios --profile production --local --clear-cache --non-interactive && mkdir -p builds/prod && mv build-* builds/prod/.",
    "build:ios:preview": "npx eas build --platform ios --profile preview --local --clear-cache --non-interactive && mkdir -p builds/preview && mv build-* builds/preview/.",
    "build:ios:dev": "npx eas build --platform ios --profile development --local --clear-cache --non-interactive && mkdir -p builds/dev && mv build-* builds/dev/.",
    "build:android:preview": "npx eas build --platform android --profile preview --local --clear-cache && mkdir -p builds/preview && mv build-* builds/preview/.",
    "build:android:dev": "npx eas build --platform android --profile development --local --clear-cache && mkdir -p builds/dev && mv build-* builds/dev/.",
    "start": "expo start --reset-cache",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@bacons/text-decoder": "^0.0.0",
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@expo/vector-icons": "^14.1.0",
    "@likashefqet/react-native-image-zoom": "^4.3.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "clsx": "^2.1.1",
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-intent-launcher": "~12.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "^5.1.4",
    "expo-secure-store": "~14.2.4",
    "expo-sqlite": "~15.2.14",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "jazz-tools": "^0.18.2",
    "nativewind": "^4.1.23",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-qr-code": "^2.0.18",
    "readable-stream": "^4.7.0",
    "reanimated-color-picker": "^4.1.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^20.0.1",
    "@types/react": "~19.0.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
